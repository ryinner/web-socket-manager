{
  "name": "@ryinner/web-socket-manager",
  "version": "0.9.0",
  "description": "simple ws manager",
  "main": "./dist/cjs/websocket.js",
  "types": "./dist/types/websocket.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/esm/websocket.js",
        "types": "./dist/types/websocket.d.ts"
      },
      "require": {
        "default": "./dist/cjs/websocket.js",
        "types": "./dist/types/websocket.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "prepublishOnly": "tsc",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryinner/webSocketManager.git"
  },
  "keywords": [
    "webSocket",
    "js",
    "typescript"
  ],
  "author": "ryinner",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ryinner/webSocketManager/issues"
  },
  "homepage": "https://github.com/ryinner/webSocketManager#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "happy-dom": "^9.20.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.31.2"
  }
}
